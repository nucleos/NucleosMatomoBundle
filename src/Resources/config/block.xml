<?xml version="1.0" encoding="UTF-8"?>
<container xmlns="http://symfony.com/schema/dic/services" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">
  <services>
    <service id="core23_matomo.block.statistic" class="Core23\MatomoBundle\Block\Service\MatomoStatisticBlockService">
      <tag name="sonata.block"/>
      <argument>core23_matomo.block.statistic</argument>
      <argument type="service" id="sonata.templating"/>
      <argument type="service" id="core23_matomo.client.factory"/>
      <call method="setLogger">
        <argument type="service" id="logger"/>
      </call>
    </service>
    <service id="core23_matomo.block.tracker" class="Core23\MatomoBundle\Block\Service\MatomoTrackerBlockService">
      <tag name="sonata.block"/>
      <argument>core23_matomo.block.tracker</argument>
      <argument type="service" id="sonata.templating"/>
    </service>
  </services>
</container>
